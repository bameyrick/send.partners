{
  "projectType": "application",
  "root": "apps/send.partners",
  "sourceRoot": "apps/send.partners/src",
  "prefix": "send.partners",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/send.partners",
        "index": "apps/send.partners/src/index.html",
        "main": "apps/send.partners/src/main.ts",
        "polyfills": "apps/send.partners/src/polyfills.ts",
        "tsConfig": "apps/send.partners/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/send.partners/src/favicon.ico",
          "apps/send.partners/src/assets",
          { "input": "libs/send-partners-common-ui/src/lib/assets", "glob": "**/*", "output": "assets" }
        ],
        "styles": ["apps/send.partners/src/styles.scss", "libs/send-partners-common-ui/src/lib/styles/index.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/send-partners-common-ui/src/lib/styles"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/send.partners/src/environments/environment.ts",
              "with": "apps/send.partners/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "send.partners:build:production"
        },
        "development": {
          "browserTarget": "send.partners:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/send.partners/proxy.conf.json",
        "port": 4300,
        "disableHostCheck": true
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "send.partners:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/send.partners/src/**/*.ts", "apps/send.partners/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/send.partners"],
      "options": {
        "jestConfig": "apps/send.partners/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
