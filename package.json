{
  "name": "app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "npm run pre-build && concurrently-await -n icons,colours,i18n,hashing,api,app,admin,storybook  \"npm run icons:watch\" \"npm run colours:watch\" \"npm run i18n:watch\" \"npm run hash-assets:watch\" \"nx serve api\" \"nx serve\" \"nx serve admin --port 4600\" \"npm run storybook\"",
    "storybook": "nx storybook common-ui",
    "build": "nx build",
    "test": "nx test --coverage",
    "lint": "nx run-many --target=lint --all && remark .",
    "tidy-clean": "ts-node build-scripts/clean.ts",
    "icons": "ts-node build-scripts/icons.ts",
    "icons:watch": "chokidar libs/common-ui/src/lib/svg/**/* -t 500 -c \"npm run icons\"",
    "colours": "ts-node build-scripts/colours.ts",
    "colours:watch": "chokidar libs/common-ui/src/lib/styles/settings/_colours.scss -t 500 -c \"npm run colours\"",
    "i18n": "ts-node build-scripts/i18n/initial.ts",
    "i18n:watch": "ts-node build-scripts/i18n/watch.ts",
    "hash-assets": "ts-node build-scripts/hashing/initial.ts",
    "hash-assets:watch": "ts-node build-scripts/hashing/watch.ts",
    "pre-build": "npm run tidy-clean && npm run icons && npm run colours && npm run i18n && npm run hash-assets",
    "pre-commit": "lint-staged",
    "prepare-husky": "husky install",
    "upgrade-interactive": "npm-check --update",
    "generate-stories": "nx g @nrwl/angular:stories common-ui"
  },
  "lint-staged": {
    "*.ts": ["prettier --print-width 140 --single-quote --trailing-comma es5 --parser typescript --write", "eslint --fix"],
    "*.scss": ["prettier --print-width 140 --single-quote --parser css --write", "sass-lint"],
    "*.json": ["prettier --print-width 140 --single-quote --trailing-comma es5 --parser json --write"],
    "*.md": ["prettier --write", "remark"]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
