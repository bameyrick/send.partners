    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-preset-angular": "11.1.1",
    "lint-staged": "^12.3.6",
    "npm-check": "^5.9.2",
    "nx": "^13.9.2",
    "prettier": "^2.5.1",
    "remark-cli": "^10.0.1",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "rimraf": "^3.0.2",
    "sass-lint": "^1.13.1",
    "storybook-dark-mode": "^1.0.9",
    "svg-sprite": "^1.5.4",
    "svgo": "^2.8.0",
    "ts-jest": "^27.1.3",
