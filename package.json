{
  "name": "send.partners",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "npm run pre-build && concurrently-await -n icons,colours,hashing,api,app,admin,storybook  \"npm run icons:watch\" \"npm run colours:watch\" \"npm run hash-assets:watch\" \"nx serve api\" \"nx serve\" \"nx serve admin.send.partners --port 4600\" \"npm run storybook\"",
    "storybook": "nx storybook send-partners-common-ui",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint && nx lint api && remark .",
    "tidy-clean": "ts-node build-scripts/clean.ts",
    "icons": "ts-node build-scripts/icons.ts",
    "icons:watch": "chokidar libs/send-partners-common-ui/src/lib/svg/**/* -t 500 -c \"npm run icons\"",
    "colours": "ts-node build-scripts/colours.ts",
    "colours:watch": "chokidar libs/send-partners-common-ui/src/lib/styles/settings/_colours.scss -t 500 -c \"npm run colours\"",
    "hash-assets": "ts-node build-scripts/hash-assets.ts",
    "hash-assets:watch": "chokidar libs/send-partners-common-ui/src/lib/raw-assets/**/* -t 500 -c \"npm run hash-assets\"",
    "pre-build": "npm run tidy-clean && npm run icons && npm run colours && npm run hash-assets",
    "pre-commit": "lint-staged",
    "prepare-husky": "husky install",
    "upgrade-interactive": "npm-check --update",
    "generate-stories": "nx g @nrwl/angular:stories send-partners-common-ui"
  },
  "lint-staged": {
    "*.ts": ["prettier --print-width 140 --single-quote --trailing-comma es5 --parser typescript --write", "eslint --fix"],
    "*.scss": ["prettier --print-width 140 --single-quote --parser css --write", "sass-lint"],
    "*.json": ["prettier --print-width 140 --single-quote --trailing-comma es5 --parser json --write"],
    "*.md": ["prettier --write", "remark"]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/throttler": "^2.0.1",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@nrwl/angular": "^13.9.2",
    "@qntm-code/utils": "^2.7.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "helmet": "^5.0.2",
    "normalize-scss": "^7.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
