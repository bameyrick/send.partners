    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "chalk": "^4.1.2",
    "chokidar-cli": "^3.0.0",
    "concurrently-await": "^1.1.4",
    "cypress": "^9.1.0",
    "eslint": "^8.11.0",
